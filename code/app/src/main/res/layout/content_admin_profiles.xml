<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="5dp"
    android:layout_margin="15dp"
    android:background="@drawable/card_background"
    android:clickable="true"
    android:focusable="true">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


            <TextView
                android:id="@+id/user_name"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_margin="20dp"
                android:textSize="16sp"
                android:fontFamily="@font/work_sans_medium"
                android:textColor="@color/dark_brown"
                android:text="Name Here"/>

            <TextView
                android:id="@+id/user_type_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/user_name"
                android:layout_marginStart="20dp"
                android:textSize="13sp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/work_sans_bold"
                android:textColor="@color/dark_brown"
                android:alpha="0.6"
                android:text="User Type: "/>

            <TextView
                android:id="@+id/user_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/user_type_title"
                app:layout_constraintTop_toBottomOf="@id/user_name"
                android:textSize="13sp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/work_sans_bold"
                android:textColor="@drawable/profile_type"
                android:alpha="0.8"
                android:text="Entrant or Organizer"/>

            <ImageButton
                android:id="@+id/menu_button"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="?android:attr/selectableItemBackground"
                android:layout_marginTop="20dp"
                android:layout_marginStart="70dp"
                android:src="@drawable/ic_3_dots_black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/user_type"
                app:layout_constraintTop_toTopOf="parent"
                android:contentDescription="Options" />



    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.appcompat.widget.LinearLayoutCompat>









